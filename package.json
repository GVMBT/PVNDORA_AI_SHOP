{
    "name": "ai-shop-platform",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "build:analyze": "vite build && echo Bundle analysis saved to dist/stats.html",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
        "typecheck": "tsc --noEmit",
        "typecheck:strict": "tsc --noEmit --project tsconfig.strict.json",
        "preview": "vite preview",
        "check:all": "npm run lint && npm run format:check && npm run typecheck"
    },
    "dependencies": {
        "@radix-ui/react-slot": "^1.2.4",
        "@resvg/resvg-js": "^2.6.2",
        "@tanstack/react-virtual": "^3.13.13",
        "@telegram-apps/sdk": "^3.11.8",
        "clsx": "^2.1.1",
        "framer-motion": "^12.23.26",
        "lucide-react": "^0.559.0",
        "react": "^19.2.1",
        "react-dom": "^19.2.1",
        "react-qr-code": "^2.0.18",
        "satori": "^0.12.0",
        "tailwind-merge": "^3.4.0",
        "tailwindcss-animate": "^1.0.7"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@types/node": "^22.14.0",
        "@types/react": "^19.1.6",
        "@types/react-dom": "^19.1.5",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.18",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "globals": "^16.5.0",
        "postcss": "^8.4.35",
        "prettier": "^3.2.5",
        "rollup-plugin-visualizer": "^6.0.5",
        "tailwindcss": "^3.4.1",
        "typescript": "~5.8.2",
        "vite": "^5.1.6"
    }
}
